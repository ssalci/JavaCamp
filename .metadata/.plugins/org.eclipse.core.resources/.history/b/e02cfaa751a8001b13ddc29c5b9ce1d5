package oopIntro;

public class Product {

	int id;
	String name;
	double unitPrice;
	String detail;
	
	public Product() {
		System.out.println("Ben çalýþtým");
	}
	
	public Product(int id, String name, double unitprice, String detail) {
		this();
		this.id=id;
		this.name=name;
		this.unitPrice=unitprice;
		this.detail=detail;
	}

}
